0, 1, 5, 23, 50

0, 1, 5, 23, 50, 70

0, 1, 5, 23, 50, 70, 10
0, 1, 5, 23, 50, 10, 70
0, 1, 5, 23, 10, 50, 70
0, 1, 5, 10, 23, 50, 70

0, 1, 5, 10, 23, 50, 70, 15

0, 1, 5, 10, 23, 50, 15, 70
0, 1, 5, 10, 23, 15, 50, 70
0, 1, 5, 10, 15, 23, 50, 70

0, 1, 5, 10, 15, 23, 50, 70, 99


// Insertion sort
var A = [5, 50, 23, 1, 70, 0, 10, 15, 99, 30];  
for (var i = 0; i < A.length; i++){
    var v = A[i], j = i-1;
    while (j >= 0 && A[j] > v){
        A[j+1] = A[j]; j--;
    }
   A[j+1] = v;
 }                    
console.log(A);

/*Работает он вот так:
5
5, 50,
5, 50, 23

5, 23, 50, 1
